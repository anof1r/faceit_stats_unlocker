{
    "manifest_version": 3,
    "name": "Faceit Statistics Unlocker",
    "description": "Unlock your stats on Faceit.com",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.faceit.com/ru/players/*/stats/cs2"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_icon": "images/default_iconF.png",
      "default_popup": "hello.html"
    }
  }